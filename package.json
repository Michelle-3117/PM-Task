{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "CI= NODE_ENV=production node ./bin/www",
    "dev": "nodemon ./bin/www",
    "dev:start": "npx npm-run-all dev:build dev:run",
    "dev:run": "NODE_ENV=development yarn nodemon",
    "build": "yarn tsc",
    "compile": "yarn tsc -w",
    "eslint": "./node_modules/eslint/bin/eslint.js --ext .ts ./src",
    "lint-fix": "yarn eslint -- --fix",
    "lint": "yarn eslint && yarn dev:build",
    "test": "jest",
    "coverage": "yarn test -- --coverage --watchAll=false "
  },
  "dependencies": {
    "axios": "^1.2.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.6.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "helmet": "^6.0.1",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "logger": "^0.0.1",
    "mongoose": "^6.8.0",
    "morgan": "~1.9.1",
    "pino": "^8.7.0",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
